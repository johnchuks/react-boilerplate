{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for react projects",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:dev": "webpack --config webpack.config.dev.js",
    "start:dev": "npm run build:dev && webpack-dev-server --config webpack.config.dev.js --progress --colors",
    "start": "webpack --config webpack.config.prod.js"
  },
  "jest": {
    "setupFiles": [
      "./tests/jestsetup.js"
    ],
    "snapshotSerializer": [
      "enxyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnchuks/react-boilerplate.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Johnbosco ohia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johnchuks/react-boilerplate/issues"
  },
  "homepage": "https://github.com/johnchuks/react-boilerplate#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "dotenv": "^6.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-jest": "^23.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "react-test-renderer": "^16.4.0",
    "webpack-cli": "^3.0.2"
  }
}
